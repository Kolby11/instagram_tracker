{
	"name": "Instagram tracker",
	"description": "Instagram tracker extension",
	"version": "1.0",
	"manifest_version": 3,
	"action": {
		"default_popup": "index.html",
		"default_icon": "instagram_tracker.png"
	},
	"permissions": ["activeTab", "webRequest"],
	"host_permissions": ["*://*.instagram.com/*"]
}
